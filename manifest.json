{
  "manifest_version": 3,
  "name": "mygo-image-searcher",
  "version": "1.0",
  "description": "Use mygo image to chat with other people",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons8-penguin-48.png",
      "48": "icons8-penguin-48.png",
      "128": "icons8-penguin-96.png"
    }
  },
  "permissions": ["clipboardWrite", "storage"]
}
